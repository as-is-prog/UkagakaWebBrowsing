{
  "manifest_version": 3,
  "name": "UkagakaWebBrowsing",
  "version": "0.1.1",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://localhost:9801/api/sstp/*"
  ],
  "action": {
    "default_title": "Send to Ghost",
    "default_icon": "icons/icon.png"
  }
}
